{
  "id": "nl.maxxie.renkforce2",
  "sdk": 2,
  "name": {
    "en": "Renkforce Remote",
    "nl": "Renkforce Afstandsbediening"
  },
  "description": {
    "en": "Add support for Renkforce Remote Control",
    "nl": "Voeg ondersteuning voor Renkforce Afstandsbediening toe"
  },
  "category": ["appliances","lights"],
  "permissions": ["homey:wireless:433"],
  "version": "2.0.0",
  "compatibility": ">=1.5.0",
  "author": {
    "name": "Max van der Helm",
    "email": "max.vanderhelm@maxxie.nl"
  },
  "images": {
    "large": "./assets/images/large.png",
    "small": "./assets/images/small.png"
  },
  "signals": {
    "433": {
      "renkforce": {
        "sof": [],
        "eof": [
          282
        ],
        "words": [
          [282,950,900,335],
          [274,958,275,955]
        ],
        "interval": 10000,
        "repetitions": 20,
        "sensitivity": 0.5,
        "minimalLength": 12,
        "maximalLength": 12
      }
    }
  },
  "flow": {},
  "drivers": [
    {
      "id": "renkforce_socket",
      "name": {
        "en": "Renkforce socket"
      },
      "class": "socket",
      "capabilities": [
        "onoff"
      ],
      "images": {
        "small": "./drivers/renkforce_socket/assets/images/small.jpg",
        "large": "./drivers/renkforce_socket/assets/images/large.jpg"
      },
      "rf": {
        "signal": "renkforce"
      },
      "settings": [],
      "pair": [
        {
          "id": "imitate",
          "rf_template": "imitate",
          "navigation": {}
        },
        {
          "id": "test_switch",
          "rf_template": "test_switch",
          "navigation": {
            "prev": "imitate",
            "next": "done"
          }
        },
        {
          "id": "done",
          "rf_template": "done"
        }
      ]
    },
    {
      "id": "renkforce_remote",
      "name": {
        "en": "Renkforce remote"
      },
      "class": "button",
      "capabilities": [
        "onoff"
      ],
      "images": {
        "small": "./drivers/renkforce_remote/assets/images/small.jpg",
        "large": "./drivers/renkforce_remote/assets/images/large.jpg"
      },
      "rf": {
        "signal": "renkforce"
      },
      "settings": [],
      "pair": [
        {
          "id": "imitate",
          "rf_template": "imitate",
          "navigation": {}
        },
        {
          "id": "test_switch",
          "rf_template": "test_switch",
          "navigation": {
            "prev": "imitate",
            "next": "done"
          }
        },
        {
          "id": "done",
          "rf_template": "done"
        }
      ]
    }
  ]
}
