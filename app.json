{
  "id": "nl.maxxie.renkforce",
  "name": {
    "en": "Renkforce"
  },
  "description": {
        "en": "Renkforce wallsocked",
        "nl": "Renkforce schakelmodule"
    },
  "category": ["appliances","lights"],
  "version": "1.0.0",
  "compatibility": ">=1.0",
  "author": {
    "name": "Max van der Helm",
    "email": "max.vanderhelm@maxxie.nl"
  },
  "images": {
    "large": "./assets/images/large.png",
    "small": "./assets/images/small.png"
  },
  "signals": {
    "433": {
      "RenkforceSignal_01": {
        "sof": [],
        "eof": [
          282
        ],
        "words": [
          [282,950,900,335],
          [274,958,275,955]
        ],
        "interval": 10000,
        "sensitivity": 0.7,
        "repetitions": 20,
        "minimalLength": 12,
        "maximalLength": 12
      }
    }
  },
  "drivers": [
    {
      "id": "renkforce_socket",
      "name": {
        "en": "Renkforce socket"
      },
      "class": "socket",
      "capabilities": [
        "onoff"
      ],
      "images": {
        "small": "./drivers/renkforce_socket/assets/images/small.jpg",
        "large": "./drivers/renkforce_socket/assets/images/large.jpg"
      },
      "pair": [
        {
          "id": "generic_choice",
          "navigation": {}
        },
        {
          "id": "generic_imitate",
          "navigation": {
            "prev": "generic_choice"
          }
        },
        {
          "id": "generic_test_switch_2",
          "navigation": {
            "prev": "generic_imitate",
            "next": "generic_choose_slave_2"
          }
        },
        {
          "id": "generic_choose_slave_2",
          "navigation": {
            "prev": "generic_test_switch_2",
            "next": "generic_done"
          },
          "template": "choose_slave"
        },
        {
          "id": "generic_program",
          "navigation": {
            "prev": "generic_choice",
            "next": "generic_test_switch"
          }
        },
        {
          "id": "generic_test_switch",
          "navigation": {
            "prev": "generic_program",
            "next": "generic_choose_slave"
          }
        },
        {
          "id": "generic_choose_slave",
          "navigation": {
            "prev": "generic_test_switch",
            "next": "generic_done"
          },
          "template": "choose_slave"
        },
        {
          "id": "generic_done",
          "navigation": {}
        }
      ]
    }
  ],
  "flow": {
    "triggers": [],
    "conditions": [],
    "actions": []
  },
  "permissions": [
    "homey:wireless:433"
  ]
}
